    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "check-types": "tsc --noEmit",
    "dev:init": "tsx ./scripts/setup-dev-env.ts --init",
    "dev:start-env": "tsx scripts/setup-dev-env --only-docker",
